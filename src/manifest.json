{
   "manifest_version": 2,
   "name": "Commander",
   "author": "Sameer Zeidat",
   "description": "Run browser and web page actions",
   "version": "0.3",
   "icons": {
      "16": "img/commander_16_color.png",
      "48": "img/commander_48_color.png",
      "128": "img/commander_128_color.png"
   },
   "background": {
      "scripts": [
         "js/background.js"
      ]
   },
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "js/content.js"
         ],
         "run_at": "document_end"
      }
   ],
   "options_ui": {
      "page": "options.html",
      "browser_style": true
   },
   "browser_action": {
      "browser_style": true,
      "default_icon": {
         "16": "img/commander_16_grey.png"
      },
      "default_title": "Commander",
      "default_popup": "actions.html"
   },
   "permissions": [
      "tabs",
      "clipboardWrite",
      "storage",
      "<all_urls>"
   ],
   "browser_specific_settings": {
      "gecko": {
         "id": "{cee9d60f-a270-41e2-85df-bbd2c4ab87ca}"
      }
   },
   "commands": {
      "_execute_browser_action": {
         "description": "Commander",
         "suggested_key": {
            "default": "F1"
         }
      },
      "command-1": {
         "description": "Action not defined"
      },
      "command-2": {
         "description": "Action not defined"
      },
      "command-3": {
         "description": "Action not defined"
      },
